{
  "sourcetype": "LaunchDarkly",
  "time": "{{timestamp.milliseconds}}",
  "event": {
    "kind": "{{kind}}",
    "action": "{{{verbKind}}}",
    "description": "{{{details.plainText}}}",
    {{#equal kind 'flag'}}
    "flag_name": "{{name}}",
    "flag_key": "{{key}}",
    {{/equal}}
    "verb": "{{titleVerb}}",
    "message": "{{{title.plainText}}}",
    {{#if project}}
    "project_name": "{{project.name}}",
    "project_key": "{{project.key}}",
    {{#if project.environment}}
    "environment_name": "{{project.environment.name}}",
    "environment_key": "{{project.environment.key}}",
    {{/if}}
    {{/if}}
    "comment": "{{#if comment}}{{{comment}}}{{/if}}",
    "user": "{{member.displayName}}",
    "user_email": "{{member.email}}",
    "url": "{{_links.details.href}}"
  }
}
