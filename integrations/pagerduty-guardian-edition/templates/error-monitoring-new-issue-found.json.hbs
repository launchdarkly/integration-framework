{
  "routing_key": "{{{formVariables.integrationKey}}}",
  "event_action": "trigger",
  "dedup_key": "{{{templateVariables.errorMonitoringDedupKey}}}",
  {{#if link}}
  "links": [
    {
      "href": "{{{link}}}",
      "text": "Go to LaunchDarkly"
    }
  ],
  {{/if}}
  "payload": {
    "summary" : "New issue found: {{{templateVariables.errorMessage}}}",
    "timestamp": "{{{timestamp}}}",
    "severity": "{{{formVariables.severity}}}",
    "source": "app.launchdarkly.com"
  }
}
